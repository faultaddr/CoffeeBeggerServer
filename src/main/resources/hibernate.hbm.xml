<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.faultaddr.coffeebeggerserver.entity.MGameEntity">
        <table name="game" schema="CoffeeBegger" />
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="participant">
                <column name="participant"/>
            </basic>
            <basic name="result">
                <column name="result" nullable="false"/>
            </basic>
            <basic name="gameId">
                <column name="gameId" nullable="false"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.faultaddr.coffeebeggerserver.entity.MUserEntity">
        <table name="user" schema="CoffeeBegger" />
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="avatar">
                <column name="avatar"/>
            </basic>
            <basic name="city">
                <column name="city" nullable="false"/>
            </basic>
            <basic name="countryCode">
                <column name="countryCode" nullable="false" length="100"/>
            </basic>
            <basic name="gender">
                <column name="gender" nullable="false" length="100"/>
            </basic>
            <basic name="nickName">
                <column name="nickName" nullable="false" length="100"/>
            </basic>
            <basic name="province">
                <column name="province" nullable="false" length="100"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
